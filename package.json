{
  "name": "laravel-connector",
  "version": "1.3.2",
  "description": "Framework-agnostic connector for Laravel APIs with Sanctum support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "laravel",
    "api",
    "sanctum",
    "csrf"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "rdehnhardt",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^20.0.7",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
